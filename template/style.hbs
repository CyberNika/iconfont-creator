
@font-face {
  font-family: "{{fontName}}";
  src: url('../fonts/{{fontName}}.eot?t={{timestamp}}'); /* IE9*/
  src: url('../fonts/{{fontName}}.eot?t={{timestamp}}#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('../fonts/{{fontName}}.woff?t={{timestamp}}') format('woff'), /* chrome, firefox */
  url('../fonts/{{fontName}}.ttf?t={{timestamp}}') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('../fonts/{{fontName}}.svg?t={{timestamp}}#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "{{fontName}}" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
{{#each icons}}

.icon-{{name}}:before { content: "\\{{hexCode}}"; }
{{/each}}
